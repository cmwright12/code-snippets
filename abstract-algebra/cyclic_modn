n = 5

def _set(x, start="...,", end=", ..."):
    output = "{" + start + str(x[0])
    for i in range(1,len(x)):
        output += ", " + str(x[i])
    output += end + "}"
    return output
    
for a in range(n):
    gen = [k*a % n for k in range(-2,n+2)]
    unique = list(set(gen))
    unique.sort()
    #print("<",a,"> = ",gen)
    #print(_set(gen))
    print(f"<{a}> = " + _set(gen) + " = " + _set(unique,"",""))
  
